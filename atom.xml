---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.name }}</title>
  <link href="http://mx.kelsin.net/"/>
  <link type="application/atom+xml" rel="self" href="http://mx.kelsin.net/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://mx.kelsin.net/</id>
  <author>
    <name>{{ site.author.name }}</name>
    <email>{{ site.author.email }}</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://mx.kelsin.net/{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://mx.kelsin.net/{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>{{ site.author.name }}</name>
      <email>{{ site.author.email }}</email>
      <uri>http://mx.kelsin.net/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
